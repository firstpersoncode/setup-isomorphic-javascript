{
  "name": "universal-react-starter",
  "version": "0.0.0",
  "description": "Universal React starter with Redux, React Router, Express, CSS/SASS Module, Mongoose, Webpack.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/firstpersoncode/universal-react-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "react-redux",
    "webpack",
    "mongoose",
    "plop",
    "sass-loader",
    "css-loader",
    "immutee",
    "isomorphic",
    "universal",
    "javascript"
  ],
  "author": "Nasser",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/firstpersoncode/universal-react-starter/issues"
  },
  "homepage": "https://github.com/firstpersoncode/universal-react-starter/blob/master/README.md",
  "scripts": {
    "start:client": "webpack-dev-server --define process.env.NODE_ENV='\"development\"' --config webpack.config.devClient.js",
    "start:server": "webpack --define process.env.NODE_ENV='\"development\"' --config webpack.config.devServer.js",
    "start": "rm -rf ./.build && npm-run-all --parallel start:client start:server",
    "build:client": "webpack --define process.env.NODE_ENV='\"production\"' --config webpack.config.prodClient.js -p",
    "build:server": "webpack --define process.env.NODE_ENV='\"production\"' --config webpack.config.prodServer.js -p",
    "build": "rm -rf ./.build && npm-run-all --parallel build:client build:server",
    "start:prod": "node ./.build/app.js",
    "generator": "cd ./utils && plop && cd ..",
    "clean": "node ./utils/clean.js"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-env": "^1.1.10",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "npm-run-all": "^4.0.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "node-fs-extra": "^0.8.1",
    "immutee": "^1.0.1",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "css-loader": "^0.25.0",
    "serve-favicon": "~2.3.0",
    "sass-loader": "^4.1.1",
    "autoprefixer": "^6.6.1",
    "react-markdown-to-html": "^1.0.11",
    "style-loader": "^0.13.1",
    "start-server-webpack-plugin": "^2.1.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "cors": "^2.8.1",
    "express": "^4.14.1",
    "history": "^4.5.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.8.1",
    "plop": "^1.7.4",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.0-beta",
    "react-hot-loader": "next",
    "react-redux": "^4.4.5",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^4.1.0",
    "redux-thunk": "^2.1.0",
    "redux-universal": "0.0.2"
  }
}
